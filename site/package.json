{
  "name": "mascp-site",
  "version": "1.0.0",
  "type": "module",
  "description": "Madison Arcatao Sister City Project website",
  "scripts": {
    "dev": "npm run build:css && npx @11ty/eleventy --serve",
    "build": "npm run build:css && npx @11ty/eleventy",
    "build:css": "npx tailwindcss -i ./src/assets/css/input.css -o ./src/assets/css/style.css --minify",
    "convert": "node scripts/convert-content.js"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "fs-extra": "^11.2.0"
  }
}
